{
  "ShowingFilterCount": {
    "message": "Showing $LOWER$-$UPPER$ of $TOTAL$ $FILTER$",
    "description": "The message displayed above the filter management panel showing the filtered item counts",
    "placeholders": {
      "lower": {
        "content": "$1"
      },
      "upper": {
        "content": "$2"
      },
      "total": {
        "content": "$3"
      },
      "filter": {
        "content": "$4"
      }
    }
  },
  "LabelFilterType": {
    "message": "Filter Type"
  },
  "LabelCreateFilter": {
    "message": "Create Filter"
  },
  "LabelRemoveFilter": {
    "message": "Remove Filter"
  },
  "LabelCreatingFilter": {
    "message": "Creating Filter..."
  },
  "LabelRemovingFilter": {
    "message": "Removing Filter..."
  },
  "LabelCreateFilterPlaceholder": {
    "message": "Create a New $FILTER$ Filter",
    "placeholders": {
      "filter": {
        "content": "$1"
      }
    }
  },
  "LabelFilteredPlaceholder": {
    "message": "Filtered $FILTER$",
    "placeholders": {
      "filter": {
        "content": "$1"
      }
    }
  },
  "LabelUnknownFilterTypePlaceholder": {
    "message": "Unknown Filter Type: $FILTER$",
    "placeholders": {
      "filter": {
        "content": "$1"
      }
    }
  },
  "CreateFilterError": {
    "message": "An error occurred while creating filter: $ERROR$",
    "description": "The error message displayed if an error occurs while creating a new filter item.",
    "placeholders": {
      "error": {
        "content": "$1"
      }
    }
  },
  "RemoveFilterError": {
    "message": "An error occurred while removing filter: $ERROR$",
    "description": "The error message displayed if an error occurs while removing a filter item.",
    "placeholders": {
      "error": {
        "content": "$1"
      }
    }
  }
